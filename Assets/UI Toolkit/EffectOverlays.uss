#Container, #Container > VisualElement
{
    position: absolute;
    width: 100%;
    height: 100%;
}

#Container > VisualElement
{
    transition-duration: 1s;
}

#Container > VisualElement:enabled
{
    opacity: 1;
    transition-timing-function: ease-in-cubic;
}
#Container > VisualElement:disabled
{
    opacity: 0;
    transition-timing-function: ease-out-cubic;
}

#Frost { background-image: url("project:/Assets/Textures/UIfrost.png"); }